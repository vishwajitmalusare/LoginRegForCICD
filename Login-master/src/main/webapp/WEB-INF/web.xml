<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>LoginServlet</servlet-name>
  	<display-name>LoginServlet</display-name>
  	<description></description>
  	<servlet-class>com.Serlvet.LoginServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>RegisterSerlvet</servlet-name>
  	<display-name>RegisterSerlvet</display-name>
  	<description></description>
  	<servlet-class>com.Serlvet.RegisterSerlvet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>ForgetServlet</servlet-name>
  	<display-name>ForgetServlet</display-name>
  	<description></description>
  	<servlet-class>com.Serlvet.ForgetServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>SetPassword</servlet-name>
  	<display-name>SetPassword</display-name>
  	<description></description>
  	<servlet-class>com.Serlvet.SetPassword</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>LoginServlet</servlet-name>
  	<url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>RegisterSerlvet</servlet-name>
  	<url-pattern>/register</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ForgetServlet</servlet-name>
  	<url-pattern>/forget</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>SetPassword</servlet-name>
  	<url-pattern>/set</url-pattern>
  </servlet-mapping>
</web-app>
